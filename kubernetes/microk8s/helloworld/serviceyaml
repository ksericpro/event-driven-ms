apiVersion: v1
kind: Service
metadata:
  labels:
    app.kubernetes.io/created-by: eric-see
    app.kubernetes.io/type: app
  name: helloworld
  namespace: ms-layer
spec:
  selector:
    app: helloworld
  ports:
    - port: 80
      protocol: TCP
      targetPort: 8080
      #nodePort: 30008
  #type: LoadBalancer
  type: ClusterIP
